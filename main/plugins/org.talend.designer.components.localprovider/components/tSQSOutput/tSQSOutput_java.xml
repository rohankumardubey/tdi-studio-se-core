      SHOW_IF="ASSUME_ROLE == 'true'"
    >
      <ITEMS DEFAULT="DEFAULT">
        	<ITEM NAME="DEFAULT" VALUE="DEFAULT" />
	        <ITEM NAME="GovCloud" VALUE="&quot;us-gov-west-1&quot;" />
