	        <ITEM NAME="CA_CENTRAL_1" VALUE="&quot;ca-central-1&quot;" />
    	</ITEMS>
    </PARAMETER> 
    
    <PARAMETER
      NAME="EXTERNALID"
      FIELD="TEXT"
      NUM_ROW="37"
      GROUP="ASSUME_ROLE_CONFIG"
      SHOW_IF="ASSUME_ROLE == 'true'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SERIALNUMBER"
      FIELD="TEXT"
      NUM_ROW="38"
      GROUP="ASSUME_ROLE_CONFIG"
      SHOW_IF="ASSUME_ROLE == 'true'"
