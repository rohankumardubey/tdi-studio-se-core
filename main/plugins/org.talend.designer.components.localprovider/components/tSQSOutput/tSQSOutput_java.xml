<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20130810A"
    STARTABLE="false"
    PARTITIONING="NONE"
    SCHEMA_AUTO_PROPAGATE="true"
    DATA_AUTO_PROPAGATE="false"
    LOG4J_ENABLED="true"
    SUPPORTS_DB_TYPE="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Cloud/Amazon/SQS</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="FLOW" NAME="MAIN" MAX_OUTPUT="1" BASE_SCHEMA="FLOW"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="0"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

    <PARAMETERS>
    <PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
        NUM_ROW="2"  GROUP="CONNECTION_CONFIG">
        <DEFAULT>false</DEFAULT>
    </PARAMETER>
    <PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" REQUIRED="true" FILTER="tSQSConnection"
        NUM_ROW="2" SHOW_IF="USE_EXISTING_CONNECTION == 'true'" GROUP="CONNECTION_CONFIG"/>

    <PARAMETER
      NAME="ACCESS_KEY"
      FIELD="TEXT"
      NUM_ROW="20"
      REQUIRED="true"
      GROUP="CONNECTION_CONFIG"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (INHERIT_CREDENTIALS == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SECRET_KEY"
      FIELD="PASSWORD"
      NUM_ROW="20"
      REQUIRED="true"
      GROUP="CONNECTION_CONFIG"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (INHERIT_CREDENTIALS == 'false')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
 	<PARAMETER
      NAME="INHERIT_CREDENTIALS"
      FIELD="CHECK"
      NUM_ROW="21"
      REQUIRED="true"
      GROUP="CONNECTION_CONFIG"
      SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>    
    
    <PARAMETER
      NAME="ASSUME_ROLE"
      FIELD="CHECK"
      NUM_ROW="22"
      REQUIRED="true"
      GROUP="CONNECTION_CONFIG"
      SHOW_IF="(USE_EXISTING_CONNECTION == 'false')" 
    >
      <DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="ARN"
      FIELD="TEXT"
      NUM_ROW="23"
      REQUIRED="true"
      GROUP="CONNECTION_CONFIG"
      SHOW_IF="isShow[ASSUME_ROLE] AND (ASSUME_ROLE == 'true')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="ROLE_SESSION_NAME"
      FIELD="TEXT"
      NUM_ROW="24"
      REQUIRED="true"
      GROUP="CONNECTION_CONFIG"
      SHOW_IF="isShow[ASSUME_ROLE] AND (ASSUME_ROLE == 'true')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
      NAME="SESSION_DURATION"
      FIELD="TEXT"
      NUM_ROW="25"
      GROUP="CONNECTION_CONFIG"
      SHOW_IF="isShow[ASSUME_ROLE] AND (ASSUME_ROLE == 'true')"
    >
      <DEFAULT>15</DEFAULT>
    </PARAMETER>   
     
    <PARAMETER 
	    NAME="REGION" 
	    FIELD="OPENED_LIST" 
	    RAW="true" 
	    NUM_ROW="26" 
	    REQUIRED="true" 
	    SHOW_IF="USE_EXISTING_CONNECTION == 'false'"
	 >
     	<ITEMS DEFAULT="DEFAULT">
        	<ITEM NAME="DEFAULT" VALUE="DEFAULT" />
         	<ITEM NAME="AP_SOUTHEAST_1" VALUE="&quot;ap-southeast-1&quot;" />
	        <ITEM NAME="AP_SOUTHEAST_2" VALUE="&quot;ap-southeast-2&quot;" />
	        <ITEM NAME="AP_NORTHEAST_1" VALUE="&quot;ap-northeast-1&quot;" />
	        <ITEM NAME="CN_NORTH_1" VALUE="&quot;cn-north-1&quot;" />
	        <ITEM NAME="EU_WEST_1" VALUE="&quot;eu-west-1&quot;" />
	        <ITEM NAME="EU_CENTRAL_1" VALUE="&quot;eu-central-1&quot;" />
	        <ITEM NAME="GovCloud" VALUE="&quot;us-gov-west-1&quot;" />
	        <ITEM NAME="SA_EAST_1" VALUE="&quot;sa-east-1&quot;" />
	        <ITEM NAME="US_EAST_1" VALUE="&quot;us-east-1&quot;" />
	        <ITEM NAME="US_WEST_1" VALUE="&quot;us-west-1&quot;" />
	        <ITEM NAME="US_WEST_2" VALUE="&quot;us-west-2&quot;" />
    	</ITEMS>
    </PARAMETER>
    
    <PARAMETER
      NAME="QUEUE_URL"
      FIELD="TEXT"
      NUM_ROW="30"
      REQUIRED="true"
    >
    <DEFAULT>""</DEFAULT>
    </PARAMETER>
    
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="50" REQUIRED="true">
        <TABLE  IF="(USE_FIFO == 'true') AND (USE_DEDUPLICATION == 'true')">
			<COLUMN KEY="false" NAME="Body" TYPE="id_String" CUSTOM="true"/>
			<COLUMN KEY="false" NAME="MessageGroupId" TYPE="id_String" CUSTOM="true"/>
		</TABLE>
	    <TABLE  IF="(USE_FIFO == 'true') AND (USE_DEDUPLICATION == 'false')">
			<COLUMN KEY="false" NAME="Body" TYPE="id_String" CUSTOM="true"/>
			<COLUMN KEY="false" NAME="MessageGroupId" TYPE="id_String" CUSTOM="true"/>
			<COLUMN KEY="false" NAME="MessageDeduplicationId" TYPE="id_String" CUSTOM="true"/>
		</TABLE>
		<TABLE IF="USE_FIFO == 'false'">
			<COLUMN KEY="false" NAME="Body" TYPE="id_String" CUSTOM="true"/>
		</TABLE>

	</PARAMETER>    
	
	<PARAMETER NAME="SCHEMA_FLOW" FIELD="SCHEMA_TYPE" NUM_ROW="50" CONTEXT="MAIN">
          <TABLE READONLY="true" IF="(USE_BATCH == 'true')">
          </TABLE>
          <TABLE READONLY="true" IF="(USE_BATCH != 'true')">
            <COLUMN NAME="MessageId" TYPE="id_String" LENGTH="255" READONLY="false" CUSTOM="true"/>
          </TABLE>
        </PARAMETER>
	
	<PARAMETER
		NAME="USE_BATCH"
		FIELD="CHECK"
		NUM_ROW="60" />

	<PARAMETER
		NAME="BATCH_SIZE"
		FIELD="TEXT"
		NUM_ROW="60"
		REQUIRED_IF="USE_BATCH == 'true'"
		SHOW_IF="USE_BATCH == 'true'"
	>
		<DEFAULT>5</DEFAULT>
	</PARAMETER>
	
	<PARAMETER NAME="USE_FIFO" FIELD="CHECK" NUM_ROW="70" />
	<PARAMETER NAME="USE_DEDUPLICATION" FIELD="CHECK" NUM_ROW="70" SHOW_IF="USE_FIFO == 'true'"/>
	  
    <PARAMETER
      NAME="DIE_ON_ERROR"
      FIELD="CHECK"
      NUM_ROW="121"
    >
      <DEFAULT>true</DEFAULT>
    </PARAMETER>
    
   </PARAMETERS>

    <ADVANCED_PARAMETERS>
